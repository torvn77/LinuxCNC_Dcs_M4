;
; Set up default formating for variables
;
; Line numbering
FORMAT = [N|@||1.0]
; Tool number
FORMAT = [T|@||1.0]
; Spindle Speed
FORMAT = [S|@||1.0]
; Feed Rate
FORMAT = [F|@||1.0]
; Tool moves in x,y and z
FORMAT = [X|@||1.4]
FORMAT = [Y|@||1.4]
FORMAT = [Z|@||1.4]
;
FORMAT = [I|@||1.8]
FORMAT = [J|@||1.8]
;
FORMAT = [SZ|@||1.2]
; Home tool positions 
FORMAT = [XH|@||1.2]
FORMAT = [YH|@||1.2]
FORMAT = [ZH|@||1.2]
;
;
FILE_EXTENSION = "ngc"
;
; Ограничение минимального размера дуги прямо пропорционально скорости движения и обратно пропорционально величине BASE_PERIOD
MIN_ARC_LENGTH=0.05
